[system]
dbType       = "mysql"
env          = "#env#"
projectId    = 0
errorMsgFile = "/data/lang.txt"

[jwt]
status      = "open"
key         = "chukong"
expiresTime = 360000
bufferTime  = 36000


[viper]
status  = "open"
watch   = "open"

[zap]
status          = "open"
level           = "info"
baseDir         = "#log_dir#"
showLine        = true
softLinkFileName= "latest_log"
LogInConsole    = true
StacktraceKey   = "stacktrace"
format          = ""
moduleName      = ""
Prefix          = ""
encodeLevel     = "LowercaseLevelEncoder"
autoAlert       = true

[redis]
status      = "open"
db-number   = 1
ip          = "#redis_ip#"
port        = "#redis_ip#"
password    =  "#redis_ps#"

[captcha]
status       = "open"
numberLength = 4
imgWidth     = 240
imgHeight    = 80


[http]
status          = "close"
ip              = "#http_ip#"
port            = "#http_port#"
staticPath      = "/data/www/golang/src/zgoframe/static"
reqLimitTimes   = 100

[mysql]
status = "open"
ip              = "#mysql_ip#"
port            = "#mysql_port#"
config          = "charset=utf8mb4&parseTime=True&loc=Local"
DbName          = "test"
username        = "#mysql_user#"
password        = "#mysql_ps#"
maxIdleConns    = 10
maxOpenConns    = 100
logMode         = true
logZap          = false

[casbin]
status      = "open"
modelPath   = "/data/www/golang/src/zgoframe/static/data/rbac_model.conf"

[etcd]
status      = "open"
ip          = "#etcd_ip#"
port        = "#etcd_port#"
username    = "#etcd_user#"
password    = "#etcd_ps#"
url         = ""

[alert]
status  = "open"
host    = "#alert_ip#"
port    = "#alert_port#"
uri     = "/api/v2/alerts"

[metrics]
status = "close"

[websocket]
status  = "close"
uri     = "/ws"

[grpc]
status                  = "open"
ip                      = "#grpc_ip#"
port                    = "#grpc_port#"
servicePackagePrefix    = "pb"

[protobuf]
basePath        ="protobuf"
pbServicePath   ="pbservice"
protoPath       ="proto"
idMapFileName   ="map.txt"

[serviceDiscovery]
status = "open"
prefix = "/service"

[email]
status  = "open"
host    = "#email_stmp#"
port    = "#email_stmp_port#"
from    = "#email_from#"
ps      = "#email_ps#"

[pushGateway]
status  = "close"
ip      = "#push_gateway_ip#"
port    = "#push_gateway_port#"