[system]
dbType       = "mysql"
projectId    = #projectId#
errorMsgFile = "/data/lang.txt"
opDirName    = "operation"

[jwt]
status      = "open"
key         = "chukong"
expiresTime = 360000

[viper]
status  = "open"
watch   = "open"

[zap]
status          = "open"
level           = "info"
baseDir         = "#log_dir#"
showLine        = true
softLinkFileName= "latest_log"
LogInConsole    = true
StacktraceKey   = "stacktrace"
format          = ""
moduleName      = ""
Prefix          = ""
encodeLevel     = "LowercaseLevelEncoder"
autoAlert       = true

[redis]
status      = "open"
db-number   = 1
ip          = "#redis_ip#"
port        = "#redis_port#"
password    =  "#redis_ps#"

[captcha]
status       = "open"
numberLength = 4
imgWidth     = 240
imgHeight    = 80


[http]
status          = "open"
ip              = "#http_ip#"
port            = "#http_port#"
#staticPath      = "/data/www/golang/src/zgoframe/static" ，这里先改成相对路径
staticPath      = "static"
reqLimitTimes   = 100

[[mysql]]
status = "open"
ip              = "#mysql_ip#"
port            = "#mysql_port#"
config          = "charset=utf8mb4&parseTime=True&loc=Local"
DbName          = "seed"
username        = "#mysql_user#"
password        = "#mysql_ps#"
maxIdleConns    = 10
maxOpenConns    = 100
logMode         = true
logZap          = false
masterSlave     = "master"

[casbin]
status      = "open"

[etcd]
status      = "open"
ip          = "#etcd_ip#"
port        = "#etcd_port#"
username    = "#etcd_user#"
password    = "#etcd_ps#"
url         = ""

[alert]
status  = "open"
host    = "#alert_ip#"
port    = "#alert_port#"
uri     = "/api/v2/alerts"

[metrics]
status = "open"

[websocket]
status  = "close"
uri     = "/ws"

[grpc]
status                  = "open"
ip                      = "#grpc_ip#"
port                    = "#grpc_port#"
servicePackagePrefix    = "pb"

[protobuf]
basePath        ="protobuf"
pbServicePath   ="pbservice"
protoPath       ="proto"
idMapFileName   ="map.txt"

[serviceDiscovery]
status = "open"
prefix = "/service"

[email]
status  = "open"
host    = "#email_stmp#"
port    = "#email_stmp_port#"
from    = "#email_from#"
ps      = "#email_ps#"
authCode = "#email_auth_code#"

[pushGateway]
status  = "close"
ip      = "#push_gateway_ip#"
port    = "#push_gateway_port#"

[gateway]
status  = "close"
ListenIp = "127.0.0.1"
OutIp    = "127.0.0.1"
WsPort   = "4444"
TcpPort  = "5555"
WsUri    = "/ws"

[cicd]
status  = "close"

[configCenter]
status  = "open"
persistenceType = 3
dataPath = "/data/www/Zgoframe/master/static/data/config"

[upload]
#注意：这里是一个相对路径，且没有：录层级，可以修改名字，但不可以增加层级（名字两边也不要加反斜杠）
#两个地方会用到它：1. cicd 2. 文件上传
#原因1：cicd 部署时：会自动创建该目录名（软连接），实际物理位置由CICD里的配置信息确定
#原因2: 文件上传类，在本地存储文件时，得有个本机的目录，存储成功后，前端的请求是：一个host+ 相对目录名+文件名，如果host是直接指向此目录，那写绝对路径也行，但是如果host只是指向的是该目录的上一级目录（如：静态资源统一域名，不区分是否为上传/proto/js/等）,那么绝对路径肯定不行
#此目录就在项目的根目录下，不需要你手动创建，CICD会帮忙处理，如果不使用CICD，需要手动创建(软连接)
path = "upload"

[oss]
accessKeyId = "#oss_user#"
accessKeySecret = "#oss_ps#"
endpoint = "#oss_ip#"
bucket="#oss_port#"
localDomain = "seedreality.com"

[domain]
static = "#domain_static_host#"