

    func (grpcManager *GrpcManager)GetFrameSyncClient(name string)(pb.FrameSyncClient,error){
        client, err := grpcManager.GetClientByLoadBalance(name,0)
        if err != nil{
            return nil,err
        }
    
        return client.(pb.FrameSyncClient),nil
    }
